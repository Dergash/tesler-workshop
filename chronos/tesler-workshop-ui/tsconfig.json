{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "target": "es5",
    "downlevelIteration": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
      "lib": [
            "dom",
            "es2016",
            "es2017.object"
    ],
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
